# AI Model API Keys (Required)
OPENAI_API_KEY=your_openai_key
ANTHROPIC_API_KEY=your_anthropic_key

# Blockchain RPC (Required)
WEB3_PROVIDER_URI=https://rpc.arc.testnet
PRIVATE_KEY=your_private_key

# Contract Addresses (After deployment)
RWA_VAULT_ADDRESS=0x...
AI_AGENT_ADDRESS=0x...
RISK_MANAGER_ADDRESS=0x...

# Supabase Database (Required)
# Get from: https://supabase.com/dashboard/project/_/settings/database
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_role_key
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.your-project.supabase.co:5432/postgres

# Upstash Redis (Required)
# Get from: https://console.upstash.com/
UPSTASH_REDIS_REST_URL=https://your-redis.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_upstash_token

# Oracle APIs (Optional)
CHAINLINK_API_KEY=your_key
COINGECKO_API_KEY=your_key
